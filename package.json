{
  "name": "af-consul",
  "version": "3.0.19",
  "description": "A highly specialized function library",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "author": "Viacheslav Makarov <vvmakarov@bazilio.ru>",
  "license": "ISC",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test:register": "ts-node ./__tests__/register.ts",
    "test:deregister": "ts-node ./__tests__/deregister.ts",
    "test:access-points": "jest --config jest.config.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "release": "np --yolo",
    "clean": "shx rm -rf dist/*",
    "build": "tsc -b tsconfig.json tsconfig.esm.json tsconfig.types.json",
    "cb": "npm run clean && npm run build"
  },
  "dependencies": {
    "async-mutex": "^0.4.0",
    "consul": "0.40.0",
    "lodash": "^4.17.21",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.4.3",
    "@types/consul": "^0.40.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@types/xxhashjs": "^0.2.2",
    "af-logger": "^2.0.28",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "eslint-config-af-22": "^1.0.11",
    "jest": "^29.4.3",
    "np": "^7.6.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/cjs/src",
    "dist/esm",
    "dist/types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bazilio-san/af-consuly.git"
  }
}
