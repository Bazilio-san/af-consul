    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "np": "^7.6.0",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tslog": "^3.3.0",
    "typescript": "^4.5.2"
