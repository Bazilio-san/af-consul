{
  "name": "af-consul",
  "version": "3.0.28",
  "description": "A highly specialized function library",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "author": "Viacheslav Makarov <vvmakarov@bazilio.ru>",
  "license": "ISC",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test:register": "ts-node ./__tests__/register.ts",
    "test:deregister": "ts-node ./__tests__/deregister.ts",
    "test:access-points": "jest --config jest.config.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "shx rm -rf dist/*",
    "build": "tsc -b tsconfig.json tsconfig.esm.json tsconfig.types.json",
    "cb": "npm run clean && npm run build"
  },
  "dependencies": {
    "@types/consul": "^0.40.1",
    "async-mutex": "^0.4.0",
    "consul": "^1.2.0",
    "lodash": "^4.17.21",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.2",
    "@types/xxhashjs": "^0.2.2",
    "af-logger": "^2.0.29",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "eslint-config-af-22": "^1.0.13",
    "jest": "^29.7.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/cjs/src",
    "dist/esm",
    "dist/types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bazilio-san/af-consul.git"
  }
